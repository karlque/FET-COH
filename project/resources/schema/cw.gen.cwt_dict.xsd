	<complexType name="agent">
		<annotation>
			<documentation>agent</documentation>
		</annotation>
			<sequence>
				<element name="userId" minOccurs="0" maxOccurs="1" type="cwf:userId"/>
				<element name="password" minOccurs="0" maxOccurs="1" type="cwt_dict:passwordClearText"/>
				<element name="role" minOccurs="0" maxOccurs="1" type="cwf_up:groupName"/>
			</sequence>
		</complexType>

	<complexType name="message">
		<annotation>
			<documentation>message</documentation>
		</annotation>
			<sequence>
				<element name="errorText" minOccurs="0" maxOccurs="1" type="xsd:string"/>
				<element name="errorValue" minOccurs="0" maxOccurs="1" type="xsd:string"/>
			</sequence>
			<attribute name="errorCode" type="xsd:string"/>
		</complexType>

	<complexType name="OCRequest">
		<annotation>
			<documentation>request</documentation>
		</annotation>
			<sequence>
				<element name="requestId" minOccurs="0" maxOccurs="1" type="cwt_dict:APIRequestID"/>
				<element name="requestType" minOccurs="0" maxOccurs="1" type="cwt_dict:APIRequestType"/>
				<element name="requestor" minOccurs="0" maxOccurs="1" type="cwt_dict:APIParticipant"/>
				<element name="echoValue" minOccurs="0" maxOccurs="1" type="xsd:string"/>
				<element name="domain" minOccurs="0" maxOccurs="1" type="cwt_dict:domain"/>
				<element name="language" minOccurs="0" maxOccurs="1" type="cwt_dict:APILanguage"/>
				<element name="details" minOccurs="0" maxOccurs="1" type="cwt_dict:APIboolean"/>
			</sequence>
		</complexType>

	<complexType name="OCResponse">
		<annotation>
			<documentation>response</documentation>
		</annotation>
			<sequence>
				<element name="requestId" minOccurs="0" maxOccurs="1" type="cwt_dict:APIRequestID"/>
				<element name="requestType" minOccurs="0" maxOccurs="1" type="cwt_dict:APIRequestType"/>
				<element name="requestor" minOccurs="0" maxOccurs="1" type="cwt_dict:APIParticipant"/>
				<element name="echoValue" minOccurs="0" maxOccurs="1" type="xsd:string"/>
				<element name="responder" minOccurs="0" maxOccurs="1" type="cwt_dict:APIParticipant"/>
				<element name="noneFound" minOccurs="0" maxOccurs="1" type="cwt_dict:APIboolean"/>
				<element name="success" minOccurs="0" maxOccurs="1" type="cwt_dict:APIboolean"/>
				<element name="message" minOccurs="0" maxOccurs="unbounded" type="cwt_dict:message"/>
			</sequence>
		</complexType>

	<complexType name="responseList">
		<annotation>
			<documentation>responseList</documentation>
		</annotation>
			<sequence>
				<element name="id" minOccurs="0" maxOccurs="1" type="cwf:cwDocId"/>
				<element name="code" minOccurs="0" maxOccurs="1" type="cwt_dict:APICode"/>
				<element name="label" minOccurs="0" maxOccurs="1" type="cwt_dict:APILabel"/>
			</sequence>
		</complexType>

