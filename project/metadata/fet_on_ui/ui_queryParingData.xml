  <methodList>
    <method name="queryParingData" type="uimthd">
      <script><![CDATA[
        /*
         *
         */

        var queryParingDataResponse = fet_on_api.queryParingDataBehavior(this.queryParingDataRequest);
        var result = queryParingDataResponse.mapTo(new Document('fet_on_ui.queryParingDataResponseDoc'), 'fet_on_ui.queryParingDataResponseDS2Doc');
        var dataObjectList = new DataObjectList();
        for (var i = 0 ; result && i < result.length ; i ++) {
            dataObjectList.addLast(result[i]);
        }

        this.queryParingDataResponseDoc = dataObjectList;

        return;
      ]]></script>
    </method>
    <method name="onInit" type="action">
      <valueType>nmeta_com.conceptwave.system.Object</valueType>
      <script><![CDATA[
        this.queryParingDataRequest = new DataStructure('fet_on_api.queryParingDataRequest');
        this.queryParingDataResponseDoc = new DataObjectList();
      ]]></script>
    </method>
    <method name="onSelected" type="uimthd">
      <script><![CDATA[
        /*
         * auther:karl
         */

        var selectedList = this.queryParingDataResponseDoc.selected;

        for (var i = 0 ; selectedList && i < selectedList.length ; i ++) {
            var selected = selectedList[i];
            this.parent.queryEligibleOffers.queryEligibleOffersRequest.hierarchyCode = selected.ProductId;
        }

        return;
      ]]></script>
    </method>
  </methodList>
</userInterface>